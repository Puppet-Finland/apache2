{
  "name": "puppetfinland-apache2",
  "version": "2.1.0",
  "author": "Puppet-Finland team",
  "summary": "Setup Apache 2 and optionally configure firewall and monit",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-apache2.git",
  "project_page": "https://github.com/Puppet-Finland/puppet-apache2",
  "dependencies": [
    {
      "name": "puppetfinland/webserver",
      "version_requirement": ">= 0.5.0"
    },
    {
      "name": "puppetfinland/monit",
      "version_requirement": ">= 0.5.0"
    },
    {
      "name": "puppetfinland/os",
      "version_requirement": ">= 0.5.7"
    },
    {
      "name": "puppetfinland/systemd",
      "version_requirement": ">= 0.1.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
